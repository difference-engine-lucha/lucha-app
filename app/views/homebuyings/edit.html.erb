<h1>Homebuying Application <br>
  <small>
    Edit the information below
  </small>
</h1>
<hr>
<div class="panel panel-success">
  <div class="panel-heading">
    <h3 class="panel-title">Homebuying Information</h3>
  </div>
  <div class="panel-body">
    <%= form_tag "/homebuyings/#{@homebuying.id}", role: "form", method: :patch do %>
      <%= render "form" %>
      <hr>
      <div class="form-group">
        <%= submit_tag "Update", :class => 'btn btn-large btn-primary' %>
      </div>
    <% end %>
  </div>
</div>