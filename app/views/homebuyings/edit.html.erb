<h1><%= I18n.t "homebuying.application" %>
</h1>
<hr>
<div class="panel panel-success">
  <div class="panel-heading">
    <h3 class="panel-title"><%= I18n.t "homebuying.information" %></h3>
  </div>
  <div class="panel-body">
    <%= form_tag "/homebuyings/#{@homebuying.id}", role: "form", method: :patch do %>
      <%= render "form" %>
      <hr>
      <div class="form-group">
        <%= submit_tag I18n.t("homebuying.update"), :class => 'btn btn-large btn-primary' %>
      </div>
    <% end %>
  </div>
</div>